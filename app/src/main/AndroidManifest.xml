<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alagu.crunch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PushupActivity"
            android:label="@string/title_activity_pushup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".calculateActivity"
            android:label="@string/title_activity_calculate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EquivActivity"
            android:label="@string/title_activity_equiv"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
